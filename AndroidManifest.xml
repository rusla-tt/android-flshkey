        </activity>
        <activity android:name=".FlashKeyHoleActivity" android:label="@string/key_hole"></activity>
        <activity android:name=".Selectkeys" android:label="@string/selectkeys"></activity>
        <activity android:name=".ResultActivity" android:label="@string/result"></activity>
